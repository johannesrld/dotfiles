# General QOL aliases
alias c=clear
alias e='$EDITOR'
alias md=mdkir -pv

# WSL is a naughty boy when it comes to keeping time, so i have to run this periodically
alias tsync="sudo ntpdate time.windows.com"

# Git aliases
alias gs="git status"
alias gc="git commit"
alias gaa="git add ."
alias dotcm="git add . && git commit"
alias gp="git push"
